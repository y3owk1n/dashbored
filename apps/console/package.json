  "devDependencies": {
    "@dashbored/eslint-config": "^0.2.0",
    "@dashbored/prettier-config": "^0.1.0",
    "@dashbored/tailwind-config": "^0.1.0",
    "@dashbored/tsconfig": "^0.1.0",
    "@types/node": "^20.8.0",
    "@types/react": "^18.2.24",
    "@types/react-dom": "^18.2.8",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.50.0",
    "prettier": "^3.0.3",
    "tailwindcss": "3.3.3",
    "typescript": "^5.2.2"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@dashbored/eslint-config/base",
      "@dashbored/eslint-config/nextjs",
      "@dashbored/eslint-config/react"
    ]
  },
  "prettier": "@dashbored/prettier-config"
