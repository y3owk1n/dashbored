{
  "name": "@dashbored/console",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "pnpm with-env next build",
    "clean": "git clean -xdf .next .turbo node_modules",
    "dev": "pnpm with-env next dev",
    "lint": "dotenv -v SKIP_ENV_VALIDATION=1 next lint",
    "format": "prettier --write \"**/*.{js,cjs,mjs,ts,tsx,md,json}\"",
    "start": "pnpm with-env next start",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@dashbored/api": "^0.1.0",
    "@dashbored/auth": "^0.1.0",
    "@dashbored/db": "^0.1.0",
    "@dashbored/hooks": "^0.1.0",
    "@dashbored/ui": "^0.1.0",
    "@dashbored/utils": "^0.1.0",
    "@t3-oss/env-nextjs": "^0.6.1",
    "@tanstack/react-query": "^4.35.7",
    "@tanstack/react-query-devtools": "^4.35.7",
    "@tanstack/react-query-next-experimental": "5.0.0-alpha.80",
    "@trpc/client": "^10.38.5",
    "@trpc/next": "^10.38.5",
    "@trpc/react-query": "^10.38.5",
    "@trpc/server": "^10.38.5",
    "dayjs": "^1.11.10",
    "next": "^13.5.3",
    "next-auth": "0.0.0-manual.b53ca00b",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "superjson": "1.13.3",
